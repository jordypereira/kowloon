{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extend",
        "extends",
        "tailwind",
        "mixin",
        "include"
      ]
    }],
    "block-no-empty": null,
    "unit-whitelist": ["em", "rem", "s", "px", "vh", "vw", "%"]
  }
}